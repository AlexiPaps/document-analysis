generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Document {
  id        String   @id @default(uuid())
  name      String
  content   String
  summary   String
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}